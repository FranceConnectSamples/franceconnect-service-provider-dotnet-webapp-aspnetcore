@model IEnumerable<DataProvider>
@{ 
    ViewData["Title"] = "Obtenir des données externes";
}

<h2>@ViewData["Title"].</h2>
@foreach (DataProvider provider in Model)
{
    <a asp-controller="Data" asp-action="GetResource" asp-route-scheme="@provider.Name" class="btn btn-default">@provider.Name</a>
}